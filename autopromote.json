{
	"channels": {
		"slow": 2.5,
		"fast": 0.5
	},
	"catalogs": {
		"testing": {
			"days": 1,
			"next": "beta"
		},
		"beta": {
			"days": 3,
			"next": "prerelease",
			"force_install_days": 2
		},
		"prerelease": {
			"days": 4,
			"next": "release",
			"force_install_days": 3
		},
		"Production": {
			"days": null,
			"next": null,
			"force_install_days": 10
		}
	},
	"denylist": {
		"Zoom": null,
		"BlueJeans": "5.4.2"
	},
	"allowlist": {
		"Teleport": "8.*"
	},
	"remove_old_catalogs": true,
	"munki_repo": "munki_repo/AzureBlob",
	"run_makecatalogs": true,
	"fields_to_copy": [
		"postinstall_script",
		"postuninstall_script",
		"blocking_applications",
		"icon_name",
		"category",
		"description",
		"developer",
		"display_name",
		"minimum_os_version"
	],
	"force_install_days": 18,
	"force_install_time": {
		"hour": 10,
		"minute": 30
	},
	"enforce_force_install_time": true,
	"force_install_denylist": [],
	"notify_teams": true,
	"output_results": true,
	"output_results_path": "results.plist",
	"logging_level": "DEBUG",
	"envfile": "~/.autopromote.env",
	"patch_tuesday": null
}